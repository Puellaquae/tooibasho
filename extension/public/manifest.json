{
    "name": "TooiBasho",
    "description": "B站专栏存档工具",
    "version": "0.3.1",
    "manifest_version": 3,
    "options_page": "app.html",
    "permissions": [
        "contextMenus",
        "storage"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "background": {
        "service_worker": "service.js",
        "type": "module"
    }
}
